 issn = {0965-9978},
 journal = {Advances in Engineering Software},
 keywords = {Cloud computing and Virtualization and Grid computing and Data center and Intrastructure as a Service and Parallel computing},
 number = {9},
 pages = {714-722},
 title = {Towards building a cloud for scientific applications},
 url = {https://www.sciencedirect.com/science/article/pii/S0965997811001141},
 volume = {42},
 year = {2011}
